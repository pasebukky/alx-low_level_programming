#include "main.h"
#include <string.h>

int main(int argc, char *argv[])
{
    if (argc != 2)
    {
        const char *wrong_message = "Wrong password\n";
        for (int i = 0; wrong_message[i] != '\0'; i++)
        {
            _putchar(wrong_message[i]);
        }
        return 1;
    }

    // Define the correct password
    const char *correct_password = "your_correct_password_here";

    if (strcmp(argv[1], correct_password) == 0)
    {
        const char *congrats_message = "Tada! Congrats\n";
        for (int i = 0; congrats_message[i] != '\0'; i++)
        {
            _putchar(congrats_message[i]);
        }
    }
    else
    {
        const char *wrong_message = "Wrong password\n";
        for (int i = 0; wrong_message[i] != '\0'; i++)
        {
            _putchar(wrong_message[i]);
        }
    }

    return 0;
}
